{
    "name": "nextworkout",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:client": "cd client && vite",
        "dev:server": "cd server && tsx watch src/server.ts",
        "build": "npm run build:client && npm run build:server",
        "build:client": "cd client && vite build",
        "build:server": "cd server && tsc",
        "start": "cd server && node dist/server.js",
        "lint": "eslint .",
        "server-type-check": "cd server && tsc --noEmit",
        "server-type-watch": "cd server && tsc --noEmit --watch",
        "client-type-check": "cd client && tsc --noEmit",
        "client-type-watch": "cd client && tsc --noEmit --watch",
        "type-check": "npm run client-type-check && npm run server-type-check",
        "type-watch": "concurrently \"npm run client-type-watch\" \"npm run server-type-watch\"",
        "preview": "vite preview"
    },
